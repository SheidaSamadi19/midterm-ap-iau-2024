# counting sentences 1 

stn = stn1 
a = int(stn.lower().count('a')) 
b = int(stn.lower().count('b')) 
c = int(stn.lower().count('c')) 
d = int(stn.lower().count('d')) 
e = int(stn.lower().count('e')) 
f = int(stn.lower().count('f')) 
g = int(stn.lower().count('g')) 
h = int(stn.lower().count('h')) 
i = int(stn.lower().count('i')) 
j = int(stn.lower().count('j')) 
k = int(stn.lower().count('k')) 
l = int(stn.lower().count('l')) 
m = int(stn.lower().count('m')) 
n = int(stn.lower().count('n')) 
o = int(stn.lower().count('o')) 
p = int(stn.lower().count('p')) 
q = int(stn.lower().count('q')) 
r = int(stn.lower().count('r')) 
s = int(stn.lower().count('s')) 
t = int(stn.lower().count('t')) 
u = int(stn.lower().count('u')) 
v = int(stn.lower().count('v')) 
w = int(stn.lower().count('w')) 
x = int(stn.lower().count('x')) 
y = int(stn.lower().count('y')) 
z = int(stn.lower().count('z'))  
 
sentenc1_letters_V = [a,i,u,e,o] 
sentenc1_letters_C = [b,c,d,f,g,h,j,k,l,m,n,p,q,r,s,t,v,w,x,y,z] 
sten1_letter = (a+b+c+d+e+f+g+h+i+j+k+l+m+n+o+p+q+r+s+t+u+v+w+x+y+z) 
vowels1 = (a + i + u + e + o) 
