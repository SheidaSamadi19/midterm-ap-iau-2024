parag1_letters_V = [a,i,u,e,o] 
parag1_letters_C = [b,c,d,f,g,h,j,k,l,m,n,p,q,r,s,t,v,w,x,y,z] 
Pr1_letter = (a+b+c+d+e+f+g+h+i+j+k+l+m+n+o+p+q+r+s+t+u+v+w+x+y+z) 
vowels = (a + i + u + e + o) 
consonant_p_1 = (Pr1_letter - vowels ) 
avg_V_p_1 = (vowels/len(parag1_letters_V)) 
avg_C_p1 = (consonant_p_1/len(parag1_letters_C)) 
print('\n') 
print("Paragraph 1 Vowels Average    :", avg_V_p_1) 
print("Paragraph 1 Consonant Average :", avg_C_p1) 
import statistics 
vari_V_p_1 = statistics.variance(parag1_letters_V) 
vari_C_p1 = statistics.variance(parag1_letters_C) 
print ("Paragraph 1 Vowels Variance    :",vari_V_p_1) 
print ("Paragraph 1 Consonant Variance :",vari_C_p1)

