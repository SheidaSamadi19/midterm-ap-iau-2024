u = int(stn.lower().count('u')) 
v = int(stn.lower().count('v')) 
w = int(stn.lower().count('w')) 
x = int(stn.lower().count('x')) 
y = int(stn.lower().count('y')) 
z = int(stn.lower().count('z'))

sentenc33_letters_V = [a,i,u,e,o] 
sentenc33_letters_C = [b,c,d,f,g,h,j,k,l,m,n,p,q,r,s,t,v,w,x,y,z]  
sten33_letter = (a+b+c+d+e+f+g+h+i+j+k+l+m+n+o+p+q+r+s+t+u+v+w+x+y+z) 
vowels33 = (a + i + u + e + o) 
consonant_S_33 = (sten33_letter - vowels33 ) 
avg_V_S_33 = (vowels33/len(sentenc33_letters_V)) 
avg_C_S33 = (consonant_S_33/len(sentenc33_letters_C)) 
print('\n') 
print("Sentences 33 Vowels Average    :", avg_V_S_33) 
print("Sentences 33 Consonant Average :", avg_C_S33) 
import statistics 
vari_V_S_33 = statistics.variance(sentenc33_letters_V) 
vari_C_S33 = statistics.variance(sentenc33_letters_C) 
print ("Sentences 33 Vowels Variance    :",vari_V_S_33) 
print ("Sentences 33 Consonant Variance :",vari_C_S33) 
 
 
 
